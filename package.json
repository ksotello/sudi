{
  "name": "ksotello",
  "version": "1.0.0",
  "description": "A front end developer tool",
  "main": "sudi.js",
  "scripts": {
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "clean": "cleandir build",
    "babelify": "babel --stage 0 src --out-file build/compiled.js --source-maps",
    "jshint": "jshint",
    "uglify": "uglifyjs build/compiled.js --compress --mangle --enclose --output build/sudi-source.min.js",
    "build": "npm run clean && npm run jshint && npm run babelify && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "sudi@git.com"
  },
  "keywords": [
    "front",
    "end",
    "development",
    "tool",
    "design",
    "designer"
  ],
  "author": "Kyle Sotello",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.3.0",
    "clean-dir": "^1.0.3",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "uglify-js": "^2.4.24"
  }
}
